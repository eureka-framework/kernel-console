{
  "name": "eureka/kernel-console",
  "description": "Kernel for Eureka Framework. Define console application kernel versions",
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",

  "authors": [
    {
      "name": "Romain Cottard",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": {
      "Eureka\\Kernel\\Console\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eureka\\Kernel\\Console\\Tests\\Unit\\": "./tests/unit/",
      "Eureka\\Kernel\\Console\\Tests\\Integration\\": "./tests/integration/"
    }
  },

  "config": {
    "sort-packages": true
  },

  "require": {
    "php": "8.1.*||8.2.*||8.3.*",
    "eureka/component-console": "^6.0",

    "symfony/config": "^5.4||^6.3",
    "symfony/dependency-injection": "^5.4||^6.3",
    "symfony/finder": "^5.4||^6.3",
    "symfony/yaml": "^5.4||^6.3"
  },

  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.40.0",
    "phpunit/phpcov": "^9.0.2",
    "phpunit/phpunit": "^10.4.2",
    "phpstan/phpstan": "^1.10.45",
    "phpstan/phpstan-phpunit": "^1.3.15",
    "symfony/cache": "^6.3.8"
  }
}
