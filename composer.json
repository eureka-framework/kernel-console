{
  "name": "eureka/kernel-console",
  "description": "Kernel for Eureka Framework. Define console application kernel versions",
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",

  "authors": [
    {
      "name": "Romain Cottard",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": {
      "Eureka\\Kernel\\Console\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eureka\\Kernel\\Http\\Tests\\": "./tests"
    }
  },

  "config": {
    "sort-packages": true
  },

  "require": {
    "php": ">=7.4",
    "eureka/component-console": "^5.0",

    "symfony/config": "^5.1",
    "symfony/dependency-injection": "^5.1",
    "symfony/finder": "^5.1",
    "symfony/yaml": "^5.1"
  },

  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^v0.7",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpunit/phpcov": "^8.2.0",
    "phpunit/phpunit": "^9.4.0",
    "squizlabs/php_codesniffer": "^3.5"
  }
}
